{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "Redis": "127.0.0.1:6379"
  },
  "JobsTimetableConfiguration": {
    "ArticlesReplicationJob" : "* */10 * * * ?"
  },
  "ArticlesReplicationJob": {
    "IsEnabled": true,
    "SourceIdentifier": "",
    "MaxSymbolsCount": 500,
    "MaxParagraphCount": 500,
    "MaxArticlesCount": 100,
    "CommitBuffer": 10
  }
}